@article{koenderink99ijcv,
	Author = {J. Koenderink and A. Van Doorn},
	File = {koenderink99ijcv.pdf:koenderink99ijcv.pdf:PDF},
	Journal = ijcv,
	Number = {2/3},
	Pages = {159-168},
	Review = {read 25/4/2012 the locally orderless representation underlies both our low-level and high-level representations (eg SIFT and BoW) Despite that locally the data is modeled orderless, it is in general far from iid due to the spatial regularity of images. Therefore, non-iid models seem useful in this context, even more so at small local scales (smaller than complete image / patch). We can derive a locally orderless representation at different spatial granularities, and model different spatial neighborhoods as iid, meaning that we assume samples in a local bag to be non-iid, but the various local-bags to be iid from each other. The FV gradient representation would be the sum of local Polya gradients, computed over the local regions Alternatively we can build a hierarchical model. At level 1 we map local features to BoW histograms computed over a limited spatial support The level1 histograms can be quantized by a mixture of Dirichlet model, into another histogram representation. We can continue doing this, each level has a growing spatial support, and quantizes the histograms from the previous level. In this case we could use a FV gradient representation over each level, where parameters are shared across the local regions in a level. Another possibility to construct a hierarchical represenatation is to quantize the FV representation of one level into a next histogram representation.},
	Title = {The Structure of Locally Orderless Images},
	Volume = {31},
	Year = {1999}}

